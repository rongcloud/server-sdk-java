	"set": {
		"docs": "https://docs.rongcloud.cn/v3/views/im/noui/guide/chatroom/manage/key/set/serverapi.html",
		"url": "chatroom/entry/set",
		"params": {
			"chatroom": {
				"chatroomId": "chatroom1",
				"userId": "userId1",
				"key": "key1",
				"value": "value1"
			}
		},
		"response": {
			"success": {
